<div class="list-group my-3">
	<% if @notes.any? %>
	<% @notes.each do |note| %>
	
	<%= link_to(note.student, class: 'list-group-item', data: { 'turbo-frame': 'modal' }, onclick: 'modalBrowser.show();') do %>
	<%#= render partial: "note", object: note %>
	<%= note.student.name %>
	<span class="float-right badge bg-secondary">bla</span>
	<br>
	<%= note.text %>
	<br>
	<small><%= time_ago_in_words(note.created_at) %> <%= t 'ago' %></small>
	<% end %>
	<% end %>
	<% end %>
</div>
