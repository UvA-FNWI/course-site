<% @updates.each do |update| %>
<h4><%= link_to update.user.name, update.user %> <span class="label label-default"><%= update.user.schedule.name %></span> <small><%= time_ago_in_words(update.updated_at) %> <%= t(:ago) %> <%= t(:by) %> <%= update.assist.name %></small></h4>
<p><%= update.note %></p>
<hr>
<% end %>
