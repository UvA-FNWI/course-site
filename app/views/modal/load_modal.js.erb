$('#modal-browser').modal('show');
<% if @header %>
$('#modal-browser-header').html("<%= j render 'modal/modal_title', header: @header %>");
<% elsif @header_partial %>
$('#modal-browser-header').html("<%= j(render @header_partial) %>");
<% end %>
$('#modal-browser-body').html("<%= j(render @partial) %>");
<% if @tooltips %>
$('[data-toggle="tooltip"]').tooltip();
<% end %>
